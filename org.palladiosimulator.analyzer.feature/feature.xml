<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.palladiosimulator.analyzer.feature"
      label="Pallaido Analyzer Framework"
      version="1.0.0.qualifier"
      plugin="org.palladiosimulator.branding"
      license-feature="org.palladiosimulator.license"
      license-feature-version="1.0.0">

   <description url="http://www.palladio-simulator.com">
      Common features for Palladio analyzers, e.g., workflow integration and completion support.
   </description>

   <requires>
      <import plugin="org.eclipse.core.runtime" version="3.10.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.emf.ecore" version="2.10.2" match="greaterOrEqual"/>
      <import plugin="de.uka.ipd.sdq.identifier" version="2.1.0" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.pcm" version="4.0.0" match="greaterOrEqual"/>
      <import plugin="de.uka.ipd.sdq.probfunction" version="2.1.0" match="greaterOrEqual"/>
      <import plugin="de.uka.ipd.sdq.stoex" version="2.2.0" match="greaterOrEqual"/>
      <import plugin="de.uka.ipd.sdq.units" version="2.1.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ocl.ecore" version="3.3.100" match="greaterOrEqual"/>
      <import plugin="de.uka.ipd.sdq.workflow" version="2.0.1" match="greaterOrEqual"/>
      <import plugin="de.uka.ipd.sdq.workflow.launchconfig" version="2.0.0" match="greaterOrEqual"/>
      <import plugin="de.uka.ipd.sdq.workflow.mdsd" version="2.0.1" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.analyzer.completions" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="de.uka.ipd.sdq.featureconfig" version="2.1.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.debug.core"/>
      <import plugin="de.uka.ipd.sdq.errorhandling" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.debug.ui" version="3.5.1" match="greaterOrEqual"/>
   </requires>

   <plugin
         id="org.palladiosimulator.analyzer.completions"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.palladiosimulator.analyzer.workflow"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
